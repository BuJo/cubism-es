{
  "name": "cubism-es",
  "version": "1.1.3",
  "description": "Cubism.js ES6 module, based on D3 V6 components",
  "main": "dist/cubism-es.js",
  "module": "dist/cubism-es.esm.js",
  "jsnext:main": "dist/cubism-es.esm.js",
  "scripts": {
    "_prebuild": "eslint src test",
    "build": "rollup -c",
    "dev": "rollup -c rollup.config.dev.mjs -w",
    "watch": "rollup -c -w",
    "pretest": "npm run build",
    "test:watch": "mocha --reporter min --watch",
    "test": "mocha",
    "test:cover": "nyc mocha",
    "prepublish": "npm test",
    "pretty": "prettier --single-quote --trailing-comma es5 --write \"{src,__{tests,demo,dist}__}/**/*.js\""
  },
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "d3-axis": "2",
    "d3-dispatch": "2",
    "d3-fetch": "2",
    "d3-format": "2",
    "d3-interpolate": "2",
    "d3-scale": "3",
    "d3-selection": "2",
    "d3-time": "2",
    "d3-time-format": "3",
    "d3-transition": "2"
  },
  "peerDependencies": {
    "d3": "^6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.24.0",
    "@babel/eslint-parser": "^7.23.10",
    "@babel/plugin-transform-classes": "^7.23.8",
    "@babel/preset-env": "^7.24.0",
    "@babel/register": "^7.23.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "eslint": "^8.57.0",
    "mocha": "^10.3.0",
    "nyc": "^15.1.0",
    "prettier": "^3.2.5",
    "rollup": "^4.12.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^1.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BigFatDog/cubism-es.git"
  },
  "keywords": [
    "Cubism",
    "Time Series",
    "ES6",
    "Data Visualization",
    "D3",
    "D3V5"
  ],
  "author": {
    "name": "Yun Xing",
    "email": "ink.xing.yun@gmail.com",
    "url": "https://github.com/BigFatDog/"
  },
  "license": "MIT",
  "homepage": "https://github.com/BigFatDog/cubism-es"
}
